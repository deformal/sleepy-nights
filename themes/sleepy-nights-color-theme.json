	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5a6a72",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords & Flow Control",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.control.import",
				"keyword.control.export",
				"keyword.control.from"
			],
			"settings": {
				"foreground": "#a89d88",
				"fontStyle": ""
			}
		},
		{
			"name": "Data Types and Storage (float, int, struct, var, const)",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#ebcb8b"
			}
		},
		{
			"name": "Functions and Method Calls",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#a88d9d",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Classes, Types, Interfaces",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.class",
				"support.type",
				"entity.name.interface",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#dfd0b8",
				"fontStyle": ""
			}
		},
		{
			"name": "Strings & Template Literals",
			"scope": [
				"string",
				"string.quoted",
				"string.template",
				"string.interpolated"
			],
			"settings": {
				"foreground": "#9a9e96"
			}
		},
		{
			"name": "Numbers, Constants (true/false/null)",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character"
			],
			"settings": {
				"foreground": "#d4a574"
			}
		},
		{
			"name": "Variables (Local/Parameters)",
			"scope": [
				"variable",
				"variable.other.readwrite",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#c1c5cc"
			}
		},
		{
			"name": "Object Properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.type.property-name.json",
				"meta.structure.dictionary.key.json"
			],
			"settings": {
				"foreground": "#d4a574"
			}
		},
		{
			"name": "JSON Values (Non-String)",
			"scope": [
				"meta.structure.dictionary.value.json constant.language.json",
				"meta.structure.dictionary.value.json constant.numeric.json"
			],
			"settings": {
				"foreground": "#bf616a"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.comparison",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#c48977"
			}
		},
		{
			"name": "HTML/XML Tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#8ab99a"
			}
		}
	]
}