	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5a6a72",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Documentation Comments",
			"scope": [
				"comment.block.documentation",
				"comment.line.documentation"
			],
			"settings": {
				"foreground": "#6a7a82",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"foreground": "#ebcb8b",
				"fontStyle": ""
			}
		},
		{
			"name": "Import/Export Keywords",
			"scope": [
				"keyword.control.import",
				"keyword.control.export",
				"keyword.control.from"
			],
			"settings": {
				"foreground": "#ebcb8b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted"
			],
			"settings": {
				"foreground": "#c1c5cc"
			}
		},
		{
			"name": "Template Strings",
			"scope": [
				"string.template",
				"string.interpolated"
			],
			"settings": {
				"foreground": "#d9c8a6"
			}
		},
		{
			"name": "String Interpolation Punctuation",
			"scope": [
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#ebcb8b"
			}
		},
		{
			"name": "Numbers, Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character"
			],
			"settings": {
				"foreground": "#d4a574"
			}
		},
		{
			"name": "Built-in Constants",
			"scope": [
				"constant.language.boolean",
				"constant.language.null",
				"constant.language.undefined"
			],
			"settings": {
				"foreground": "#d4a574",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other.readwrite",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#c1c5cc"
			}
		},
		{
			"name": "Object Properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#dfd0b8"
			}
		},
		{
			"name": "This/Self Keywords",
			"scope": [
				"variable.language.this",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#ebcb8b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#bf616a",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Method Calls",
			"scope": [
				"meta.method-call",
				"meta.function-call.method"
			],
			"settings": {
				"foreground": "#c47419"
			}
		},
		{
			"name": "Classes, Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#c4a882",
				"fontStyle": ""
			}
		},
		{
			"name": "Interfaces, Namespaces",
			"scope": [
				"entity.name.interface",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#b89872",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML/XML Tags",
			"scope": [
				"entity.name.tag",
				"meta.tag.structure",
				"meta.tag.inline"
			],
			"settings": {
				"foreground": "#8ab99a"
			}
		},
		{
			"name": "HTML/XML Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#c4a882"
			}
		},
		{
			"name": "CSS Selectors",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#c4a882"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name.css",
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#c47419"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value.css",
				"meta.property-value.css"
			],
			"settings": {
				"foreground": "#c9b896"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#8a8e96"
			}
		},
		{
			"name": "Brackets",
			"scope": [
				"meta.brace",
				"punctuation.definition.block",
				"punctuation.section"
			],
			"settings": {
				"foreground": "#9a9e96"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.comparison",
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#c48977"
			}
		},
		{
			"name": "Assignment Operators",
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#bf616a"
			}
		},
		{
			"name": "Invalid/Deprecated",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#e85d5d",
				"fontStyle": "bold underline"
			}
		},
		{
			"name": "Deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#c87848",
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#a89d88",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#dfd0b8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#c8b9a1",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.underline.link",
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#8a9d9d",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markdown Code",
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#b8a890",
				"background": "#222831"
			}
		},
		{
			"name": "Markdown Quote",
			"scope": [
				"markup.quote"
			],
			"settings": {
				"foreground": "#7a7e86",
				"fontStyle": "italic"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json",
				"meta.structure.dictionary.key.json"
			],
			"settings": {
				"foreground": "#ebcb8b"
			}
		},
		{
			"name": "JSON Values",
			"scope": [
				"meta.structure.dictionary.value.json"
			],
			"settings": {
				"foreground": "#eceff4"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#8a9d9d"
			}
		},
		{
			"name": "Regex Special Characters",
			"scope": [
				"constant.character.escape",
				"constant.other.character-class.regexp"
			],
			"settings": {
				"foreground": "#a89d88",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#a89d88",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Support Constants",
			"scope": [
				"support.constant"
			],
			"settings": {
				"foreground": "#b89d88"
			}
		}
